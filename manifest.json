{

  "manifest_version": 2,
  "name": "LinkedIn Withdrawer",
  "description": "Withdraws all not yet accepted invitations on Linkedin.",
  "version": "1.1",

  "browser_action": {
    "default_icon": {
      "19": "icon19.png",
      "38": "icon38.png"
    },
    "default_title":"LinkedIn Withdrawer",
    "default_popup": "popup.html"
  },

  "permissions": [
    "https://secure.flickr.com/"
  ]
}
